{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "baseUrl": {
            "type": "string",
            "defaultValue": "https://raw.githubusercontent.com/marcvaneijk/azure-resource-manager-context/master/context"
        }
    },
    "variables": {},
    "resources": [
        {
            "name": "getContext",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2015-01-01",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[concat(parameters('baseUrl'),'/linked/context.json')]",
                    "contentVersion": "1.0.0.0"
                }
            }
        }
    ],
    "outputs": {
        "cloud": {
            "value": "[reference('getContext').outputs.cloud.value]",
            "type": "string"
        }
    }
}